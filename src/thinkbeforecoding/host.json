{
  "version": "2.0",
  "logging": {
    "logLevel": { "default": "Information" },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 1
      }
    },
    "fileLoggingMode": "never"
  },
  "aggregator": {
    "batchSize": 1000,
    "flushTimeout": "00:01:00"
  }
}